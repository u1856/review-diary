<div class="like-buttn">
<% if movie.liked_by?(current_user) %>
  <%= link_to "", movie_likes_path(movie), method: :delete, remote: true, class: "fas fa-bookmark like-btn" %></td>
<% else %>
  <%= link_to "", movie_likes_path(movie), method: :post, remote: true, class: "fas fa-bookmark unlike-btn" %></td>
<% end %>
</div>